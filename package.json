{
  "name": "@dodo-framework/dodo-headless",
  "version": "0.0.1",
  "scripts": {
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "svelte-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "check": "pnpm run svelte-check && eslint . && pnpm run lint",
    "check:fix": "pnpm run svelte-check && eslint . && pnpm run format",
    "dev": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-svelte-csf": "^5.0.0-next.23",
    "@storybook/blocks": "^8.5.0",
    "@storybook/svelte": "^8.5.0",
    "@storybook/sveltekit": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-netlify": "^4.4.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "publint": "^0.3.2",
    "sass": "^1.83.4",
    "storybook": "^8.5.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^5.4.11"
  },
  "volta": {
    "node": "22.13.1",
    "pnpm": "10.0.0"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "nanoid": "^5.0.9"
  },
  "packageManager": "pnpm@10.0.0"
}
